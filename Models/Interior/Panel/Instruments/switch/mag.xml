<?xml version="1.0" encoding="ISO-8859-1"?> 

<PropertyList>

  <path>mag.ac</path>

  <animation>
    <object-name>base</object-name>
    <object-name>Switch0</object-name>
    <object-name>Switch1</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>base</object-name>
    <object-name>Switch0</object-name>
    <object-name>Switch1</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>Switch0</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/engine[0]/mag0</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>pick</type>
    <object-name>Switch1</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <binding>
        <command>property-toggle</command>
        <property>/controls/engines/engine[0]/mag1</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>Switch0</object-name>
    <property>/controls/engines/engine[0]/mag0</property>
    <factor> 80 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>Switch1</object-name>
    <property>/controls/engines/engine[0]/mag1</property>
    <factor> 80 </factor>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
